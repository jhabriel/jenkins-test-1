// First attempt to create a Jenkinsfile

pipeline{

    agent any

    stages {

        stage("build") {

            steps {
                echo 'building porepy ...'
            }
        }

        stage("test") {

            steps {
                echo 'testing porepy ...'
            }
        }

        stage("deploy") {

            steps {
                echo 'deploying porepy ...'
            }
        }

    }
}